{
  "name": "@ehacke/eslint-config",
  "version": "1.0.0",
  "description": "General purpose Node ESLint configuration",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehacke/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "Eric Hacke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ehacke/eslint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "prettier *.js --write"
    }
  },
  "files": [
    "*.js",
    "!scratch.*"
  ],
  "dependencies": {
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-no-secrets": "^0.3.4",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "eslint-plugin-xss": "^0.1.9"
  },
  "peerDependencies": {
    "@ehacke/prettier-config": "^1.0.1",
    "eslint": "^5.13.0"
  },
  "devDependencies": {
    "@ehacke/commitlint-config": "^1.0.1",
    "@ehacke/prettier-config": "^1.0.1",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "fixpack": "^2.3.1",
    "husky": "^4.2.3"
  },
  "homepage": "https://github.com/ehacke/eslint-config#readme"
}
